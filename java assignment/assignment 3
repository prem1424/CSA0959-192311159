package javaprojectp;

public class helloworld {
	static public void main(String[] args) {
		System.out.println("  ****  ");
		System.out.println(" *    * ");
		System.out.println("* *  * *");
		System.out.println("*      *");
		System.out.println("* *  * *");
		System.out.println("*  **  *");
		System.out.println(" *    * ");
		System.out.println("  ****  ");
		
	}

}
OUTPUT:
  ****  
 *    * 
* *  * *
*      *
* *  * *
*  **  *
 *    * 
  ****  
